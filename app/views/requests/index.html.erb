<p id="notice"><%= notice %></p>

<h1>Requests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th>Date</th>
      <th>Thirtyl</th>
      <th>Thirtyq</th>
      <th>Fiftyl</th>
      <th>Fiftyq</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.name %></td>
        <td><%= request.phone %></td>
        <td><%= request.email %></td>
        <td><%= request.address %></td>
        <td><%= request.date %></td>
        <td><%= request.thirtyl %></td>
        <td><%= request.thirtyq %></td>
        <td><%= request.fiftyl %></td>
        <td><%= request.fiftyq %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_request_path %>
